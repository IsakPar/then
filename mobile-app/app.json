{
  "expo": {
    "name": "Last Minute Live",
    "slug": "last-minute-live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lml-appstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "lastminutelive",
    "splash": {
      "image": "./assets/lml-appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2937"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.YOUR_IOS_CLIENT_ID"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/lml-appstore.png",
      "bundleIdentifier": "com.isakpar.lastminutelive",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.YOUR_IOS_CLIENT_ID"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lml-appstore.png",
        "backgroundColor": "#F7D92A"
      },
      "edgeToEdgeEnabled": true,
      "icon": "./assets/lml-appstore.png",
      "package": "com.isakpar.lastminutelive",
      "config": {
        "googleSignIn": {
          "apiKey": "YOUR_ANDROID_API_KEY",
          "certificateHash": "YOUR_SHA1_FINGERPRINT"
        }
      }
    },
    "web": {
      "favicon": "./assets/lml-appstore.png"
    },
    "extra": {
      "apiUrl": "https://then-production.up.railway.app",
      "eas": {
        "projectId": "your-expo-project-id"
      }
    }
  }
}
